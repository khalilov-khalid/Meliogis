                <th>
                    @Html.DisplayNameFor(model => model.PROPERTY.NAME)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.PROPERTY_TYPE.NAME)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.REGION.Adi)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.REGION1.Adi)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.REGION2.Adi)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.SIB.NAME)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.TECHNICAL_TYPE.NAME)
                </th>
                <th></th>
            </tr>

        </thead>

        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.FID_CHANNELS)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.NAME)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.WATER_CAPABILITY)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.EXPLONATION_DATE)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.WATERPROOF_WIDTH)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.DEVICE_SUM)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.FACTICAL_LENGTH)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.GIS_LENGTH)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.NOTE)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.SERVED_AREAHA)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.SHAPE_STLength__)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.ACTIVITY.NAME)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.ASSIGMENT.NAME)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.CHANNEL_TYPES.NAME)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.CHANNEL1.NAME)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.COVER_TYPES.NAME)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.KIND.NAME)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.PROPERTY.NAME)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.PROPERTY_TYPE.NAME)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.REGION.Adi)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.REGION1.Adi)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.REGION2.Adi)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.SIB.NAME)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.TECHNICAL_TYPE.NAME)
                    </td>
                    <td>
                        <a href="@Url.Action("Edit", "OpenChannel", new { id = item.OBJECTID })" class="ico ico_edit"><i class="fas fa-pencil-alt"></i></a>
                    </td>
                </tr>
            }

        </tbody>

    </table>

    @Html.PagedListPager((IPagedList)Model, page => Url.Action("Index", new { page, type = ViewBag.Type }))

</div>