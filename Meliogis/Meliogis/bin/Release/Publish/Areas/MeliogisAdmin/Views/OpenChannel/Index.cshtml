@using X.PagedList.Mvc;
@using X.PagedList;
@model IEnumerable<Meliogis.Models.CHANNEL>
@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/MeliogisAdmin/Views/Shared/AdminLayout.cshtml";
}

<div class="table_con">
    <table class="table table-hover table-bordered th_fixed">
        <thead>
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.FID_CHANNELS)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.NAME)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.WATER_CAPABILITY)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.EXPLONATION_DATE)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.WATERPROOF_WIDTH)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.DEVICE_SUM)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.FACTICAL_LENGTH)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.GIS_LENGTH)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.NOTE)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.SERVED_AREAHA)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.SHAPE_STLength__)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.ACTIVITY.NAME)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.ASSIGMENT.NAME)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.CHANNEL_TYPES.NAME)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.CHANNEL1.NAME)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.COVER_TYPES.NAME)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.KIND.NAME)
