@model Meliogis.ViewModels.IndexViewModel
@{
    ViewBag.Title = "Home Page";
}


<!-- Header -->
<header class="header clear w-100p">

    <a href="@Url.Action("Index","Home")" title="Meliogis" class="logo float-left" lang="en">
        <strong>
            Meliogis
        </strong>
    </a>

    <div class="search-form relative bg-white float-left tr-3s">

        <div class="input-group">

            <input type="search" id="search-input" class="input no-border" placeholder="Axtar">

            <span class="input-group-btn">

                <button type="button" id="_testSearch" class="btn bg-white no-border" aria-label="Axtar">
                    <i aria-hidden="true" class="icon-search"></i>
                </button>

                <button type="button" class="search-toggle btn bg-white no-border hide-lg hide-md hide-sm" aria-label="Bağla">
                    <i aria-hidden="true" class="icon-close"></i>
                </button>

            </span>
            <!-- Left ToolBar -->
            <div class="drop map-change float-left ml-5">
                <span role="button"
                      class="drop-toggle btn bg-white no-border caret mr-5"
                      id="map-menu"
                      aria-label="Xəritələr"
                      aria-haspopup="true"
                      aria-expanded="false">
                    <i aria-hidden="true" class="icon-map"></i>
                </span>
                <div class="drop-menu static animated fadeInUp mt-5" aria-labelledby="map-menu">
                    <div class="menu-header">
                        <strong>Xəritə tipi</strong>
                    </div>
                    <div class="menu-body">
                        <div class="ckbox">
                            <input type="radio" name="map-type" id="topo" checked onclick="MapFunction('topo')">
                            <label for="topo">Əsas xəritə</label>
                        </div>
                        <div class="ckbox">
                            <input type="radio" name="map-type" id="streets" onclick="MapFunction('streets')">
                            <label for="streets">OSM</label>
                        </div>
                        <div class="ckbox">
                            <input type="radio" name="map-type" id="satellite" onclick="test(this)">
                            <label for="satellite">Bing</label>
                        </div>
                    </div>
                </div>
            </div>

        </div>

        <div id="_searchContect" class="search-filter animated fadeInUp mt-5 bg-white shadow w-100p light scroll" role="dialog" hidden>

            <div class="filter-list" role="group" aria-hidden="true" hidden>
                <ul class="type-none m-0 p-0" role="listbox" aria-hidden="true">
                    <li role="listitem" class="pt-7 pb-7 pl-15 pr-15 pointer">
                        <span class="line-camp line-1">
                            <i aria-hidden="true" class="icon-map-pin mr-10"></i>
                            <span>Search result</span>
                        </span>
                    </li>
                </ul>
            </div>

            <div class="empty-result text-center text-gray pt-40 pb-40 border-top" role="presentation" aria-hidden="true" hidden>
                <p class="h1"><i aria-hidden="true" class="fa fa-search"></i></p>
                <span>Uyğun nəticə tapılmadı</span>
            </div>

        </div>


    </div>

    <button style="display:none" type="button" class="search-toggle btn bg-white no-border float-left ml-5 hide-lg hide-md hide-sm" aria-label="Bağla">
        <i aria-hidden="true" class="icon-search"></i>
    </button>

    <button style="display:none" class="full-screen btn bg-white no-border float-left ml-5" title="İstifadə qaydaları" data-target-modal="terms-of-use">
        <i aria-hidden="true" class="icon-info-circle-thin"></i>
    </button>

</header>

<!-- Sidebar menu -->
<aside class="main-sidebar bg-white shadow tr-3s" aria-labelledby="side-menu">

    <span role="button"
          aria-hidden="true"
          aria-label="Side toggle"
          class="side-toggle btn bg-white no-border float-right"
          aria-haspopup="true"
          aria-expanded="false" id="side-menu">
        <i aria-hidden="true" class="v-icon icon-menu-nav"></i>
        <i aria-hidden="true" class="h-icon icon-close"></i>
    </span>


    <div class="sidebar-body scroll o-auto" data-tab="network">

        <div class="pt-20 pl-20 pr-20 bg-gray clear">
            <div class="row as-5">
                <div class="col group mb-20">
                    <label for="select-district-0">Rayonlar</label>
                    <div class="drop district-menu w-100p">
                        <input role="button" class="drop-toggle input caret _inputFilter" id="district-menu" aria-label="Rayonlar" aria-haspopup="true" aria-expanded="true" placeholder="Seç" />
                        <div class="drop-menu static animated fadeInUp mt-5" aria-labelledby="district-menu">
                            <ul class="menu-body">
                                <li class="menu-item">
                                    <div class="ckbox">
                                        <input type="checkbox" class="_regions" name="rayon" id="0">
                                        <label for="0">Hamısı</label>
                                    </div>
                                </li>
                                @foreach (var item in Model._regions)
                                {
                                    <li class="menu-item">
                                        <div class="ckbox">
                                            <input type="checkbox" class="_regions" name="rayon" id="@item.OBJECTID">
                                            <label for="@item.OBJECTID">@item.NAME</label>
                                        </div>
                                    </li>
                                }
                            </ul>
                        </div>
                    </div>
                </div>
                <div class="col group mb-20">
                    <label for="select-district-0">Kəndlər</label>
                    <div class="drop district-menu w-100p">
                        <input role="button" class="drop-toggle input caret _inputFilter" id="village-menu" aria-label="Kəndlər" aria-haspopup="true" aria-expanded="true" placeholder="Seç" />
                        <div class="drop-menu right static animated fadeInUp mt-5" aria-labelledby="village-menu">
                            <ul class="menu-body" id="_villiageContent">
                                <!-- scriptle yigilir bu hisse  -->
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <!-- idareler ve sibler hissesi-->
        <div class="pt-20 pl-20 pr-20 border-bottom clear">
            <div class="row as-5">

                <div class="col group mb-20">
                    <label for="select-institution">İdarələr</label>
