@model IEnumerable<Meliogis.GetJsonModel.BUlDINGMODEL>


<table class="work-planning-table table bordered mb-50">

    <tr>
        <th>
            Adı
            @*@Html.DisplayNameFor(model => model.NAME)*@
        </th>
        <th>
            Aktivlik
            @*@Html.DisplayNameFor(model => model.ACTION)*@
        </th>
        <th>
            Tətinat
            @*@Html.DisplayNameFor(model => model.ASSIGMENT_ID)*@
        </th>
        <th>
            Mühafizə növü
            @*@Html.DisplayNameFor(model => model.PROTECTING)*@
        </th>
        <th>
            Ümumi sahəsi
            @*@Html.DisplayNameFor(model => model.TOTAL_AREA)*@
        </th>
        <th>
            İstifadə rejimi
            @*@Html.DisplayNameFor(model => model.USING_MODE)*@
        </th>
        <th>
            Xidmət sahəsi
            @*@Html.DisplaykNameFor(model => model.SERVICE_AR)*@
        </th>
        <th>
            SİB
            @*@Html.DisplayNameFor(model => model.SIBS_ID)*@
        </th>
        <th>
            Istismara verildiyi tarix
            @*@Html.DisplayNameFor(model => model.EXPLONATIO)*@
        </th>
        <th>
            Activlik
            @*@Html.DisplayNameFor(model => model.ACTIVITY.NAME)*@
        </th>
        <th>
            Mülkiyyətçi
            @*@Html.DisplayNameFor(model => model.PROPERTY.NAME)*@
        </th>
        <th>
            Mülkiyyət növü
            @*@Html.DisplayNameFor(model => model.PROPERTY_TYPE.NAME)*@
        </th>
        <th>
            Kendin Adı
            @*@Html.DisplayNameFor(model => model.REGION.Adi)*@
        </th>
        <th>
            Rayonun Adı
            @*@Html.DisplayNameFor(model => model.REGION1.Adi)*@
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NAME)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ACTION)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ASSIGMENT_NAME)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PROTECTING)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TOTAL_AREA)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.USING_MODE)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SERVICE_AR)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SIBS_NAME)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EXPLONATION)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ACTIVITY_NAME)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SSI_NAME)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PROPERTY_TYPE_NAME)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.VILLAGE_NAME)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.REGIONS_NAME)
            </td>
        </tr>
    }
</table>

<div class="pag_con">
    <div class="con">
        <button class="_DetalsReport" data-skip="0" data-name="@Model.ToList().FirstOrDefault().DataName">İlk səhifə</button>
        @if (ViewBag.CP + 5 <= 11)
        {
            for (int i = 1; i <= 11; i++)
            {
                <button class="_DetalsReport @(ViewBag.CP == i ? "active" : "")" data-skip="@(i-1)" data-name="@Model.ToList().FirstOrDefault().DataName">@i</button>
            }
        }
        else if (ViewBag.CP + 5 > 11 && ViewBag.CP + 5 <= ViewBag.PC)
        {
            for (int i = ViewBag.CP - 5; i <= ViewBag.CP + 5; i++)
            {
                <button class="_DetalsReport @(ViewBag.CP == i ? "active" : "")" data-skip="@(i-1)" data-name="@Model.ToList().FirstOrDefault().DataName">@i</button>
            }
        }
        else if (ViewBag.CP + 5 > ViewBag.PC)
        {
            for (int i = ViewBag.CP - (10 - ((int)ViewBag.PC - ViewBag.CP)); i <= ViewBag.PC; i++)
            {
                <button class="_DetalsReport @(ViewBag.CP == i ? "active" : "")" data-skip="@(i-1)" data-name="@Model.ToList().FirstOrDefault().DataName">@i</button>
            }
        }
        <button class="_DetalsReport" data-skip="@(ViewBag.PC - 1)" data-name="@Model.ToList().FirstOrDefault().DataName">Son səhifə</button>
    </div>
</div>