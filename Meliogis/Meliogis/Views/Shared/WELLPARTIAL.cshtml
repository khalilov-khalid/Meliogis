        </th>
        <th>
            Suvarılan ərazi
            @*@Html.DisplayNameFor(model => model.IRRIGATED_AREA)*@
        </th>
        <th>
            Dərinlik
            @*@Html.DisplayNameFor(model => model.DEPTH)*@
        </th>
        <th>
            Nömrə
            @*@Html.DisplayNameFor(model => model.NUMBER_)*@
        </th>
        <th>
            Məhsuldarlığı
            @*@Html.DisplayNameFor(model => model.PRODUCTIVITY)*@
        </th>
        <th>
            Təhlükəsizlik növü
            @*@Html.DisplayNameFor(model => model.SECURITY_TYPE)*@
        </th>
        <th>
            Qeyd
            @*@Html.DisplayNameFor(model => model.NOTE)*@
        </th>
        @*<th>
                @Html.DisplayNameFor(model => model.SHAPE)
            </th>*@
        <th>
            İstismara verildiyi tarix
            @*@Html.DisplayNameFor(model => model.EXPLONATION_DATE)*@
        </th>
        <th>
            Aktivlik
            @*@Html.DisplayNameFor(model => model.ACTIVITY.NAME)*@
        </th>
        <th>
            Təyinat
            @*@Html.DisplayNameFor(model => model.ASSIGMENT.NAME)*@
        </th>
        <th>
            Mülkiyyətçisi
            @*@Html.DisplayNameFor(model => model.PROPERTY.NAME)*@
        </th>
        <th>
            Mülkiyyət növü
            @*@Html.DisplayNameFor(model => model.PROPERTY_TYPE.NAME)*@
        </th>
        <th>
            Kəndin adı
            @*@Html.DisplayNameFor(model => model.REGION.Adi)*@
        </th>
        <th>
            Rayonun adı
            @*@Html.DisplayNameFor(model => model.REGION1.Adi)*@
        </th>
        <th>
            SİB
            @*@Html.DisplayNameFor(model => model.SIB.NAME)*@
        </th>
        <th>
            Texniki vəziyyəti
            @*@Html.DisplayNameFor(model => model.TECHNICAL_TYPE.NAME)*@
        </th>
        <th>
            Quyunun Tipi
            @*@Html.DisplayNameFor(model => model.WELL_TYPE.NAME)*@
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NAME)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IRRIGATED_AREA)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DEPTH)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NUMBER_)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PRODUCTIVITY)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SECURITY_TYPE_ID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NOTE)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EXPLONATION_DATE)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ACTIVITY_NAME)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ASSIGMENT_NAME)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SSI_NAME)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PROPERTY_TYPE_NAME)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.VILLAGE_NAME)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.REGIONS_NAME)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SIBS_NAME)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TECHNICAL_TYPE_NAME)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.WELL_TYPE_NAME)
            </td>
        </tr>
    }
</table>

<div class="pag_con">
    <div class="con">
        <button class="_DetalsReport" data-skip="0" data-name="@Model.ToList().FirstOrDefault().DataName">İlk səhifə</button>
        @if (ViewBag.CP + 5 <= 11)
        {
            for (int i = 1; i <= 11; i++)
            {
                <button class="_DetalsReport @(ViewBag.CP == i ? "active" : "")" data-skip="@(i-1)" data-name="@Model.ToList().FirstOrDefault().DataName">@i</button>
            }
        }
        else if (ViewBag.CP + 5 > 11 && ViewBag.CP + 5 <= ViewBag.PC)
        {
            for (int i = ViewBag.CP - 5; i <= ViewBag.CP + 5; i++)
            {
                <button class="_DetalsReport @(ViewBag.CP == i ? "active" : "")" data-skip="@(i-1)" data-name="@Model.ToList().FirstOrDefault().DataName">@i</button>
            }
        }
        else if (ViewBag.CP + 5 > ViewBag.PC)
        {
            for (int i = ViewBag.CP - (10 - ((int)ViewBag.PC - ViewBag.CP)); i <= ViewBag.PC; i++)
            {
                <button class="_DetalsReport @(ViewBag.CP == i ? "active" : "")" data-skip="@(i-1)" data-name="@Model.ToList().FirstOrDefault().DataName">@i</button>
            }
        }
        <button class="_DetalsReport" data-skip="@(ViewBag.PC - 1)" data-name="@Model.ToList().FirstOrDefault().DataName">Son səhifə</button>
    </div>
</div>