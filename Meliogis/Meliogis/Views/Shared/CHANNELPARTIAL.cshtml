@model IEnumerable<Meliogis.GetJsonModel.ChannelsModel>


<table class="work-planning-table table bordered mb-50">
    <thead>
        <tr>
            <th>
                Adı
                @*@Html.DisplayNameFor(model => model.NAME)*@
            </th>
            <th>
                Su buraxma qabiliyyəti
                @*@Html.DisplayNameFor(model => model.WATER_CAPABILITY)*@
            </th>
            <th>
                Istismara verildiyi tarix
                @*@Html.DisplayNameFor(model => model.EXPLONATION_DATE)*@
            </th>
            <th>
                Su mühafizə zonasının eni
                @*@Html.DisplayNameFor(model => model.WATERPROOF_WIDTH)*@
            </th>
            <th>
                Üzərindəki qurğuların sayı
                @*@Html.DisplayNameFor(model => model.DEVICE_SUM)*@
            </th>
            <th>
                Uzunluğu
                @*@Html.DisplayNameFor(model => model.FACTICAL_LENGTH)*@
            </th>
            <th>
                Coğrafi uzunluğu
                @*@Html.DisplayNameFor(model => model.GIS_LENGTH)*@
            </th>
            <th>
                Qeyd
                @*@Html.DisplayNameFor(model => model.NOTE)*@
            </th>
            <th>
                Xidmet etdiyi sahe
                @*@Html.DisplayNameFor(model => model.SERVED_AREAHA)*@
            </th>
            <th>
                Xerite Uzunluğu
                @*@Html.DisplayNameFor(model => model.SHAPE_STLength__)*@
            </th>
            <th>
                Aktivlik
                @*@Html.DisplayNameFor(model => model.ACTIVITY.NAME)*@
            </th>
            <th>
                Təyinat
                @*@Html.DisplayNameFor(model => model.ASSIGMENT.NAME)*@
            </th>
            <th>
                Tipi
                @*@Html.DisplayNameFor(model => model.CHANNEL_TYPES.NAME)*@
            </th>
            <th>
                Menbe
                @*@Html.DisplayNameFor(model => model.CHANNEL1.NAME)*@
            </th>
            <th>
                Örtüyü
                @*@Html.DisplayNameFor(model => model.COVER_TYPES.NAME)*@
            </th>
            <th>
                Təyinatı
                @*@Html.DisplayNameFor(model => model.KIND.NAME)*@
            </th>
            <th>
                Mülkiyyətçi
                @*@Html.DisplayNameFor(model => model.PROPERTY.NAME)*@
            </th>
            <th>
                Mülkiyyət növü
                @*@Html.DisplayNameFor(model => model.PROPERTY_TYPE.NAME)*@
            </th>
            <th>
                Rayonun adı
                @*@Html.DisplayNameFor(model => model.REGION.Adi)*@
            </th>
            <th>
                Kəndin adı
                @*@Html.DisplayNameFor(model => model.REGION1.Adi)*@
            </th>
            <th>
                Xidmet etdiyi region
                @*@Html.DisplayNameFor(model => model.REGION2.Adi)*@
            </th>
            <th>
                Texniki vəziyyəti
                @*@Html.DisplayNameFor(model => model.TECHNICAL_TYPE.NAME)*@
            </th>
        </tr>

    </thead>

    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.NAME)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.WATER_CAPABILITY)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.EXPLONATION_DATE)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.WATERPROOF_WIDTH)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.DEVICE_SUM)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.FACTICAL_LENGTH)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.GIS_LENGTH)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.NOTE)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.SERVED_AREAHA)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.SHAPE_STLength__)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.ACTIVITY_NAME)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.ASSIGMENT_NAME)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.TYPE_NAME)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.SOURCE_NAME)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.COVER_TYPE_NAME)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.KIND_NAME)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.SSI_NAME)
                <td>
                    @Html.DisplayFor(modelItem => item.PROPERTY_TYPE_NAME)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.REGIONS_NAME)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.VILLAGE_NAME)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.SERVED_REGION_NAME)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.TECHNICAL_NAME)
                </td>
            </tr>
        }
    </tbody>
</table>

<div class="pag_con">
    <div class="con">
        <button class="_DetalsReport" data-skip="0" data-name="@Model.ToList().FirstOrDefault().DataName">İlk səhifə</button>
        @if (ViewBag.CP + 5 <= 11)
        {
        for (int i = 1; i <= 11; i++)
        {
        <button class="_DetalsReport @(ViewBag.CP == i ? " active" : "" )" data-skip="@(i-1)" data-name="@Model.ToList().FirstOrDefault().DataName">@i</button>
        }
        }
        else if (ViewBag.CP + 5 > 11 && ViewBag.CP + 5 <= ViewBag.PC)
        {
        for (int i = ViewBag.CP - 5; i <= ViewBag.CP + 5; i++)
        {
        <button class="_DetalsReport @(ViewBag.CP == i ? " active" : "" )" data-skip="@(i-1)" data-name="@Model.ToList().FirstOrDefault().DataName">@i</button>
        }
        }
        else if (ViewBag.CP + 5 > ViewBag.PC)
        {
        for (int i = ViewBag.CP - (10 - ((int)ViewBag.PC - ViewBag.CP)); i <= ViewBag.PC; i++)
        {
        <button class="_DetalsReport @(ViewBag.CP == i ? " active" : "" )" data-skip="@(i-1)" data-name="@Model.ToList().FirstOrDefault().DataName">@i</button>
        }
        }
        <button class="_DetalsReport" data-skip="@(ViewBag.PC - 1)" data-name="@Model.ToList().FirstOrDefault().DataName">Son səhifə</button>
    </div>
</div>